[
  {
    "label": "Debug main.go",
    "adapter": "Delve",
    "request": "launch",
    "mode": "exec",
    "program": "${ZED_WORKTREE_ROOT}/main_exec",
    "build": {
      "command": "bash",
      "args": [
        "-c",
        "templ generate && sqlc generate && go build -gcflags=\"all=-N -l\" -o main_exec main.go"
      ]
    }
  }
]